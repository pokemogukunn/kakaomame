{
  "version": 2,
  "name": "minecraft-like-game",
  "builds": [
    {
      "src": "/app/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "main.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/app/(.*)",
      "dest": "/app/index.html"
    }
  ]
}
